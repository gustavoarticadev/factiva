<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="product_uom_tree_view_inherit_factiva_integracion" model="ir.ui.view">
        <field name="name">product.uom.tree.view.inherit.factiva.integracion</field>
        <field name="model">product.uom</field>
        <field name="inherit_id" ref="product.product_uom_tree_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='uom_type']" position="after">
                <field name="code"/>
            </xpath>
        </field>
    </record>

    <record id="product_uom_form_view_inherit_factiva_integracion" model="ir.ui.view">
        <field name="name">product.uom.form.view.inherit.factiva.integracion</field>
        <field name="model">product.uom</field>
        <field name="inherit_id" ref="product.product_uom_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='rounding']" position="after">
                <field name="code"/>
            </xpath>
        </field>
    </record>

</odoo>